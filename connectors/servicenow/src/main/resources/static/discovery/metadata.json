{
  "image": {
    "href": "https://s3.amazonaws.com/vmw-mf-assets/connector-images/hub-servicenow.png"
  },
  "config": {
    "file_ticket_table_name": {
      "default": "task",
      "label": {
        "en-US": "Ticket table name"
      },
      "type": "STRING",
      "description": {
        "en-US": "Table name for the general ticket filing flow."
      },
      "validators": [
        {
          "type": "required",
          "description": {
            "en-US": "Ticket table name is required"
          }
        }
      ]
    }
  },
  "object_types": {
    "card": {
      "doc": {
        "href": "https://github.com/vmware-samples/card-connectors-guide/wiki/Card-Responses"
      },
      "fields": {
        "ticket_id": {
          "capture_group": 1,
          "regex": "\\b(REQ[0-9]{7})\\b"
        },
        "email": {
          "env": "USER_EMAIL"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/cards/requests"
      }
    },
    "botDiscovery": {
      "endpoint": {
        "href": "${CONNECTOR_HOST}/bot-discovery"
      }
    }
  }
}