{
  "authorization_header": "x-concur-authorization",
  "backend_base_url_header": "x-concur-base-url",
  "object_type": "card",
  "fields": {
    "concur_automated_email_subject": {
      "regex": "Expense Report Pending Your Approval"
    },
    "expense_report_id": {
      "capture_group": 1,
      "regex": "\\b([A-F0-9]{20,})\\b"
    }
  }
}
