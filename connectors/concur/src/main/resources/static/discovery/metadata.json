{
  "authorization_header": "x-concur-authorization",
  "backend_base_url_header": "x-concur-base-url",
  "object_types": [
    {
      "object_type": {
        "name": "card",
        "doc": {
          "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
        }
      },
      "fields": {
        "expense_report_id": {
          "capture_group": 1,
          "regex": "Report\\s*Id\\s*:\\s*([A-Za-z0-9]{20,})"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/cards/requests"
      }
    }
  ]
}
