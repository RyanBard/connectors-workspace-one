{
  "authorization_header": "x-bitbucket-server-authorization",
  "backend_base_url_header": "x-bitbucket-server-base-url",
  "object_types": [
    {
      "object_type": {
        "name": "card",
        "doc": {
          "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
        }
      },
      "fields": {
        "pr_email_subject": {
          "capture_group": 1,
          "regex": "((?:[a-zA-Z0-9]+)\/(?:[a-zA-Z0-9-]+) - Pull request #(?:[0-9]+):[ ])"
        }
      },
      "endpoint": {
        "href": "CONNECTOR_HOST/cards/requests"
      }
    }
  ]
}
