{
  "authorization_header": "x-jira-authorization",
  "backend_base_url_header": "x-jira-base-url",
  "object_types": [
    {
      "object_type": {
        "name": "card",
        "doc": {
          "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
        }
      },
      "fields": {
        "issue_id": {
          "capture_group": 1,
          "regex": "\\b([A-Z]{1,10}-\\d+)\\b"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/cards/requests"
      }
    }
  ]
}
